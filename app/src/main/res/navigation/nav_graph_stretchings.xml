<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_stretching"
    app:startDestination="@id/stretchingFragment">

    <fragment
        android:id="@+id/stretchingFragment"
        android:name="com.mmfsin.noexcuses.presentation.stretching.StretchingFragment"
        android:label="stretchingFragment"
        tools:layout="@layout/fragment_stretching">
        <action
            android:id="@+id/action_mGroups_to_stretchingDetail"
            app:destination="@id/stretchingDetailFragment">
            <argument
                android:name="mGroupId"
                app:argType="string" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/stretchingDetailFragment"
        android:name="com.mmfsin.noexcuses.presentation.stretching.detail.StretchingDetailFragment"
        android:label="stretchingDetailFragment"
        tools:layout="@layout/fragment_stretching" />

</navigation>